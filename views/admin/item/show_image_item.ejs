<div
  class="tab-pane fade show active"
  id="show-image"
  role="tabpanel"
  aria-labelledby="home-tab"
>
  <div class="row">
    <% for (let x = 0; x < item.imageId.length; x++) { %>
    <div class="col-md-4">
      <div class="card shadow mb-4 mt-2">
        <div class="card-body">
          <img
            src="http://localhost:3000/<%= item.imageId[x].imageUrl %>"
            alt="<%= item.imageId[x].imageUrl %>"
            style="width: 100%; display: block"
          />
        </div>
      </div>
    </div>
    <% } %>
  </div>
  <button onclick="goBack()" class="btn btn-primary">Go Back</button>
</div>

<script>
  function goBack() {
    window.history.back();
  }
</script>
